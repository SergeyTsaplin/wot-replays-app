{
    "name": "wot-replays-app",
    "productName": "wot-replays-app",
    "version": "0.1.0",
    "description": "App to work with replays from World Of Tanks",
    "main": "src/index.js",
    "scripts": {
      "start": "electron-forge start",
      "package": "electron-forge package",
      "make": "electron-forge make",
      "publish": "electron-forge publish"
    },
    "keywords": [],
    
    "license": "MIT",
    "config": {
      "forge": {
